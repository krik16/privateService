<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans          
    http://www.springframework.org/schema/beans/spring-beans.xsd          
    http://code.alibabatech.com/schema/dubbo          
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<!-- 生成远程服务代理，可以和本地bean一样使用demoService -->

	<!-- 发送佣金变化状态 -->
	<dubbo:reference id="rOACommodityCommissionService"
		interface="com.rongyi.rss.mallshop.order.ROACommodityCommissionService"
		version="${dubbo.version}" check="false" />

	<!-- 获取系统参数 -->
	<dubbo:reference id="rmmmSettingsService"
		interface="com.rongyi.rss.gcc.RmmmSettingsService" version="${dubbo.version}"
		check="false" />

	<!-- 生成提现流水号 -->
	<dubbo:reference interface="com.rongyi.rss.rpb.OrderNoGenService"
		id="orderNoGenService" version="${dubbo.version}" check="false" />

	<!-- redis -->
	<dubbo:reference interface="com.rongyi.rss.malllife.roa.ROARedisService"
					 id="rOARedisService" version="${dubbo.version}" check="false" />
</beans>  