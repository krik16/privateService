			</exclusions>
		</dependency>
		<!-- dubbo end -->

		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
		</dependency>

	</dependencies>

	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
				<excludes>
					<exclude>${excludePropertyFiles}</exclude>
				</excludes>
			</resource>
		</resources>
	</build>
	
	<properties>
	  <excludePropertyFiles></excludePropertyFiles>
	</properties>
	
	<profiles>
		<profile>
			<!-- 本地开发环境 -->
			<id>dev</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<excludePropertyFiles></excludePropertyFiles>
			</properties>
		</profile>

		<profile>
			<!-- 正式环境 -->
			<id>test</id>
			<properties>
				<excludePropertyFiles>maven_build/**/*</excludePropertyFiles>
			</properties>
		</profile>
	</profiles>
</project>